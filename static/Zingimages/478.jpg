<script>
        // Create a clone of the menu, right next to original.
        // http://codepen.io/senff/pen/ayGvD
    $('nav').addClass('original').clone().insertAfter('nav').addClass('cloned').css('position','fixed').css('top','0').css('width','100%').css('background','#ffffff');
    $('.cloned').removeClass('original');

    stickMenu();

    jQuery( window ).scroll(function() {
        stickMenu();
    });


    function stickMenu() {
      
        var menu = document.querySelector('nav');
        var menupos = menu.offsetTop;

       if (jQuery(window).scrollTop() >= menupos) {
         // scrolled past the menu; only show the cloned, sticky menu.
         jQuery('.cloned').show();
         jQuery('.original').css('visibility','hidden');
       } else {
         // not scrolled past the menu; only show the original menu.
         jQuery('.cloned').hide();
         jQuery('.original').css('visibility','visible');
       }
    }
</script>




